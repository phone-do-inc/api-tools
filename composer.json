{
  "name": "riverwaysoft/api-tools",
  "description": "Api tools",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "mitalcoi",
      "email": "victor@riverwaysoft.com"
    }
  ],
  "scripts": {
    "ci": "vendor/bin/phpstan analyse src tests",
    "tests": "vendor/bin/phpunit tests",
    "cs": "vendor/bin/php-cs-fixer fix src"
  },
  "suggest": {
    "doctrine/orm": "^2.9",
    "api-platform/core": "^2.6",
    "symfony/messenger": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "Riverwaysoft\\ApiTools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Riverwaysoft\\ApiTools\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "ext-intl": "*",
    "giggsey/libphonenumber-for-php": "^8.11",
    "myclabs/php-enum": "^1.7",
    "moneyphp/money": "^3",
    "symfony/intl": "^5.3",
    "symfony/polyfill-uuid": "^1.23",
    "phpdocumentor/reflection-docblock": "^5.2"
  },
  "require-dev": {
    "phpstan/phpstan": "0.12.91",
    "doctrine/orm": "^2.9",
    "api-platform/core": "^2.6",
    "friendsofphp/php-cs-fixer": "^3.0",
    "symfony/messenger": "^5.3",
    "symfony/validator": "^5.3",
    "phpunit/phpunit": "^9.5",
    "spatie/phpunit-snapshot-assertions": "^4.2"
  }
}
