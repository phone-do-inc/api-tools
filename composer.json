{
  "name": "riverwaysoft/api-tools",
  "description": "Api tools",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "mitalcoi",
      "email": "victor@riverwaysoft.com"
    }
  ],
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse src tests",
    "test": "vendor/bin/phpunit tests",
    "cs": "vendor/bin/php-cs-fixer fix src"
  },
  "suggest": {
    "doctrine/orm": "^2.10",
    "api-platform/api-pack": "^3.0",
    "symfony/messenger": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Riverwaysoft\\ApiTools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Riverwaysoft\\ApiTools\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.3",
    "ext-intl": "*",
    "giggsey/libphonenumber-for-php": "^8.11",
    "myclabs/php-enum": "^1.7",
    "moneyphp/money": "^3",
    "symfony/intl": "^6.4|^7.0",
    "symfony/polyfill-uuid": "^1.23",
    "phpdocumentor/reflection-docblock": "^5.2",
    "symfony/serializer": "^7.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.9",
    "friendsofphp/php-cs-fixer": "^3.0",
    "doctrine/orm": "^3.2.1",
    "symfony/messenger": "^6.4|^7.0",
    "api-platform/core": "^3.3",
    "symfony/validator": "^6.4|^7.0",
    "phpunit/phpunit": "^9.5",
    "spatie/phpunit-snapshot-assertions": "^4.2"
  }
}
